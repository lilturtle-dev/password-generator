{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "22"
    }
  },
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs22.x"
    }
  }
} 